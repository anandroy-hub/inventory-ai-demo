# Example secrets configuration for Streamlit
# Copy this file to secrets.toml and fill in your actual values
# NEVER commit secrets.toml to version control!

# Enable Hugging Face models (set to true to use HF Inference API)
ENABLE_HF_MODELS = true

# Hugging Face API token
# Obtain from: https://huggingface.co/settings/tokens
# Must start with "hf_" and be at least 20 characters
HF_TOKEN = "hf_your_token_here"
